  Launch      memo   memo -o $Experiment.log -l4
              [ -z "$launch_err" ] &&
                msg "Running script interact"
  Launch      tm_bfr bfr -s Both
  Launch      -TMC-  lgr -N `mlf_find LOG` -n lgr
  Launch      tm_gen DPOPScol
  Launch      cmd    DPOPSsrvr -s Both
  Launch      -      tmdf
  Launch      -      alicat -a 1:MFC1,2:MFC2 -p /dev/ttyUSB0
  Launch      -      POPS
  Launch      -      DPOPSalgo -v
