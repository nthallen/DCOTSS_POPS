  Launch      memo   memo -o $Experiment.log -l4
              [ -z "$launch_err" ] &&
                msg "Running script interact"
  Launch      tm_bfr bfr -s Both -v
  Launch      -TMC-  lgr -N `mlf_find LOG` -n lgr
  Launch      tm_gen DPOPScol -v
  Launch      cmd    DPOPSsrvr -s Both -v
  Launch      -      tmdf
  Launch      -      alicat -a 1:MFC1,2:MFC2 -p /dev/ttyUSB0 -v
  Launch      -      POPS -vv
  Launch      -      DPOPSalgo -v
