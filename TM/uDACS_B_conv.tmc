AD7770_T10K100KU Ring3T; Invalidate Ring3T;
{ Ring3T = (uDACS_B_Raw3 & 0xFFFFFF) |
    ((uDACS_B_Raw3 & 0x800000) ? 0xFF000000 : 0);
  Validate Ring3T;
