AD7770_T10K100KU Ring2T; Invalidate Ring2T;
{ Ring2T = (uDACS_B_Raw2 & 0xFFFFFF) |
    ((uDACS_B_Raw2 & 0x800000) ? 0xFF000000 : 0);
  Validate Ring2T;
